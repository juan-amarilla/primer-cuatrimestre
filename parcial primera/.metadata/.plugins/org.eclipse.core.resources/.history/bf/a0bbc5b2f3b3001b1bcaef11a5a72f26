typedef struct
{
   int idServicio;
   char descripcion[25];
   int precio;
   int isEmpty;

}eServicio;

void mostrarListadoDeServicios(eServicio lista[], int tam);
void mostrarUnServicio(eServicio unServicio);
void totalPesosDeServicio(eServicio lista[], int tam);

#ifndef SERVICIO_H_
#define SERVICIO_H_



#endif /* SERVICIO_H_ */
